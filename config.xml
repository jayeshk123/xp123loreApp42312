        <param name="ios-package" value="Calendar" />
    </feature>
    <feature name="cordova-plugin-armsoft-googlemaps" source="npm">
        <param name="API_KEY_FOR_ANDROID" value="AIzaSyCT7dlV_X6Q4XVQVHTY7ekBodBI1UzE70w" />
        <param name="API_KEY_FOR_IOS" value="AIzaSyA6fwxQSbHeC4prBHv_VaeEI1ZyGJC-Cn8" />
    </feature>
    <feature name="StatusBar">
        <param name="ios-package" onload="true" value="CDVStatusBar" />
    </feature>
    <feature name="CDVWKWebViewEngine">
        <param name="ios-package" value="CDVWKWebViewEngine" />
    </feature>
